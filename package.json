{
    "name": "corner-kick",
    "version": "1.0.0",
    "description": "A visualizer for the UBC Thunderbots team",
    "main": "./build/main.js",
    "scripts": {
        "start": "webpack-dev-server --config ./config/webpack.dev.js --open",
        "build": "webpack --config ./config/webpack.prod.js",
        "prod": "http-server ./build -o",
        "test": "jest --config ./config/jest.config.js",
        "test:watch": "yarn test --watch",
        "test:coverage": "yarn test --coverage",
        "storybook": "start-storybook -p 9001 -c storybook"
    },
    "author": "Quentin Golsteyn",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "7.3.4",
        "@storybook/addon-actions": "5.0.2",
        "@storybook/addon-links": "5.0.2",
        "@storybook/addon-notes": "5.0.3",
        "@storybook/addons": "5.0.2",
        "@storybook/react": "5.0.2",
        "@types/jest": "23.3.12",
        "@types/react": "16.8.7",
        "@types/react-dom": "16.8.2",
        "@types/storybook__addon-notes": "4.0.2",
        "@types/storybook__react": "4.0.1",
        "@types/styled-components": "^4.1.19",
        "babel-loader": "8.0.5",
        "clean-webpack-plugin": "1.0.0",
        "css-loader": "2.0.0",
        "error-overlay-webpack-plugin": "0.1.6",
        "file-loader": "2.0.0",
        "friendly-errors-webpack-plugin": "1.7.0",
        "html-loader": "0.5.5",
        "html-webpack-plugin": "3.2.0",
        "http-server": "0.11.1",
        "jest": "23.6.0",
        "style-loader": "0.23.1",
        "ts-jest": "23.10.5",
        "ts-loader": "5.3.1",
        "tslint": "5.11.0",
        "typescript": "3.2.2",
        "webpack": "4.29.6",
        "webpack-cli": "3.1.2",
        "webpack-dashboard": "2.0.0",
        "webpack-dev-server": "3.2.1"
    },
    "dependencies": {
        "@blueprintjs/core": "^3.18.1",
        "@blueprintjs/icons": "^3.10.0",
        "react": "16.8.4",
        "react-dom": "16.8.4",
        "styled-components": "^4.3.2"
    }
}
